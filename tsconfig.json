{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "dist",
		"lib": [
			"ES2022"
		],
		"strict": true,
		"alwaysStrict": true,
		"strictNullChecks": true,
		"declaration": true,
		"sourceMap": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"rootDir": "src",
		"noFallthroughCasesInSwitch": true,
	},
	"include": [
		"src"
	]
}